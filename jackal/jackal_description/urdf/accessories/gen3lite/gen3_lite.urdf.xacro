<?xml version="1.0"?>

<robot name="gen3_lite_gen3_lite_2f" xmlns:xacro="http://ros.org/wiki/xacro">

    <!-- Arguments -->
    <xacro:arg name="arm" default="gen3_lite" />
    <xacro:arg name="dof" default="6" />
    <xacro:arg name="vision" default="false" />
    <xacro:arg name="robot_ip" default="192.168.1.10" />
    <xacro:arg name="username" default="admin" />
    <xacro:arg name="password" default="admin" />
    <xacro:arg name="port" default="10000" />
    <xacro:arg name="port_realtime" default="10001" />
    <xacro:arg name="session_inactivity_timeout_ms" default="60000" />
    <xacro:arg name="connection_inactivity_timeout_ms" default="2000" />
    <xacro:arg name="gripper" default="gen3_lite_2f" />
    <xacro:arg name="gripper_joint_name" default="right_finger_bottom_joint" />
    <xacro:arg name="gripper_max_velocity" default="100.0" />
    <xacro:arg name="gripper_max_force" default="100.0" />
    <xacro:arg name="sim_gazebo" default="false" />
    <xacro:arg name="use_internal_bus_gripper_comm" default="true" />
    <xacro:arg name="use_external_cable" default="false" />
    <xacro:arg name="moveit_active" default="false" />
    <xacro:property name="prefix" value="kinova_" />

    <!-- <xacro:include filename="$(find kortex_description)/robots/kortex_robot.xacro" /> -->

    <xacro:macro name="kinova_gen3_lite" params="*origin parent">
        <joint name="${prefix}base_joint" type="fixed">
            <xacro:insert_block name="origin" />
            <parent link="${parent}"/>
            <child link="${ns}${prefix}base_link" />
        </joint>

        <link name="${ns}${prefix}base_link">
            <inertial>
                <origin xyz="0.00244324 0.00015573 0.08616742" rpy="0 0 0" />
                <mass value="1.14608471" />
                <inertia ixx="0.00335854" ixy="3.9E-07" ixz="0.00010989" iyy="0.003311" iyz="1.91E-06" izz="0.00077158" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/base_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/base_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}base_link">
            <material>Kortex/Gray</material>
        </gazebo>

        <link name="${ns}${prefix}shoulder_link">
            <inertial>
                <origin xyz="2.477E-05 0.02213531 0.09937686" rpy="0 0 0" />
                <mass value="0.95974404" />
                <inertia ixx="0.00165947" ixy="2E-08" ixz="3.6E-07" iyy="0.00140355" iyz="0.00034927" izz="0.00089493" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/shoulder_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/shoulder_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}shoulder_link">
            <material>Kortex/Gray</material>
        </gazebo>

        <joint name="${prefix}joint_1" type="revolute">
            <origin xyz="0 0 0.12825" rpy="0 0 0" />
            <parent link="${ns}${prefix}base_link" />
            <child link="${ns}${prefix}shoulder_link" />
            <axis xyz="0 0 1" />
            <limit lower="-2.68" upper="2.68" effort="10" velocity="1.6" />
        </joint>

        <link name="${ns}${prefix}arm_link">
            <inertial>
                <origin xyz="0.02998299 0.21154808 0.0453031" rpy="0 0 0" />
                <mass value="1.17756164" />
                <inertia ixx="0.01149277" ixy="1E-06" ixz="1.6E-07" iyy="0.00102851" iyz="0.00140765" izz="0.01133492" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/arm_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/arm_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}arm_link">
            <material>Kortex/Gray</material>
        </gazebo>

        <joint name="${prefix}joint_2" type="revolute">
            <origin xyz="0 -0.03 0.115" rpy="1.5708 0 0" />
            <parent link="${ns}${prefix}shoulder_link" />
            <child link="${ns}${prefix}arm_link" />
            <axis xyz="0 0 1" />
            <limit lower="-2.61" upper="2.61" effort="14" velocity="1.6" />
        </joint>

        <link name="${ns}${prefix}forearm_link">
            <inertial>
                <origin xyz="0.0301559 0.09502206 0.0073555" rpy="0 0 0" />
                <mass value="0.59767669" />
                <inertia ixx="0.00163256" ixy="7.11E-06" ixz="1.54E-06" iyy="0.00029798" iyz="9.587E-05" izz="0.00169091" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/forearm_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/forearm_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}forearm_link">
            <material>Kortex/Gray</material>
        </gazebo>

        <joint name="${prefix}joint_3" type="revolute">
            <origin xyz="0 0.28 0" rpy="-3.1416 0 0" />
            <parent link="${ns}${prefix}arm_link" />
            <child link="${ns}${prefix}forearm_link" />
            <axis xyz="0 0 1" />
            <limit lower="-2.61" upper="2.61" effort="10" velocity="1.6" />
        </joint>
        
        <link name="${ns}${prefix}lower_wrist_link">
            <inertial>
                <origin xyz="0.00575149 0.01000443 0.08719207" rpy="0 0 0" />
                <mass value="0.52693412" />
                <inertia ixx="0.00069098" ixy="2.4E-07" ixz="0.00016483" iyy="0.00078519" iyz="7.4E-07" izz="0.00034115" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/lower_wrist_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/lower_wrist_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}lower_wrist_link">
            <material>Kortex/Gray</material>
        </gazebo>

        <joint name="${prefix}joint_4" type="revolute">
            <origin xyz="0 -0.14 0.02" rpy="1.5708 0 0" />
            <parent link="${ns}${prefix}forearm_link" />
            <child link="${ns}${prefix}lower_wrist_link" />
            <axis xyz="0 0 1" />
            <limit lower="-2.6" upper="2.6" effort="7" velocity="1.6" />
        </joint>

        <link name="${ns}${prefix}upper_wrist_link">
            <inertial>
                <origin xyz="0.08056517 0.00980409 0.01872799" rpy="0 0 0" />
                <mass value="0.58097325" />
                <inertia ixx="0.00021268" ixy="5.21E-06" ixz="2.91E-06" iyy="0.00106371" iyz="1.1E-07" izz="0.00108465" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/upper_wrist_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/upper_wrist_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}upper_wrist_link">
            <material>Kortex/Gray</material>
        </gazebo>

        <joint name="${prefix}joint_5" type="revolute">
            <origin xyz="0.0285 0 0.105" rpy="0 1.5708 0" />
            <parent link="${ns}${prefix}lower_wrist_link" />
            <child link="${ns}${prefix}upper_wrist_link" />
            <axis xyz="0 0 1" />
            <limit lower="-2.53" upper="2.53" effort="7" velocity="1.6" />
        </joint>

        <link name="${ns}${prefix}end_effector_link"/>
        <joint name="${prefix}joint_6" type="revolute">
            <origin xyz="-0.105 0 0.0285" rpy="0 -1.5708 0" />
            <parent link="${ns}${prefix}upper_wrist_link" />
            <child link="${ns}${prefix}end_effector_link" />
            <axis xyz="0 0 1" />
            <limit lower="-2.6" upper="2.6" effort="7" velocity="3.2" />
        </joint>

        <link name="${ns}${prefix}dummy_link" />
        <joint name="${prefix}end_effector" type="fixed">
            <origin xyz="0 0 0" rpy="0 0 0" />
            <parent link="${ns}${prefix}end_effector_link" />
            <child link="${ns}${prefix}dummy_link" />
            <axis xyz="0 0 1" />
            <limit lower="0" upper="0" effort="0" velocity="0" />
        </joint>

        <!-- Tool frame used by the arm -->
        <link name="${ns}${prefix}tool_frame"/>
        <joint name="${prefix}tool_frame_joint" type="fixed">
            <origin xyz="0 0 0.130" rpy="0 0 1.57079632679" />
            <parent link="${ns}${prefix}dummy_link" />
            <child link="${ns}${prefix}tool_frame" />
            <axis xyz="0 0 0" />
        </joint>

        <!-- Gripper description -->
        <joint name="${prefix}gripper_base_joint" type="fixed">
            <origin xyz="0 0 0" rpy="0 0 0" />
            <parent link="${ns}${prefix}dummy_link" />
            <child link="${ns}${prefix}gripper_base_link" />
            <axis xyz="0 0 0" />
        </joint>

        <link name="${ns}${prefix}gripper_base_link">
            <inertial>
                <origin xyz="4.03E-06 1.08E-05 0.041397" rpy="0 0 0" />
                <mass value="0.1395" />
                <inertia ixx="0.00011614" ixy="1E-08" ixz="4E-08" iyy="0.00010327" iyz="0" izz="9.707E-05" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/gripper_base_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/gripper_base_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}gripper_base_link">
            <material>Kortex/Gray</material>
        </gazebo>

        <link name="${ns}${prefix}right_finger_prox_link">
            <inertial>
                <origin xyz="0.020257 0.0063483 6.991E-05" rpy="0 0 0" />
                <mass value="0.018385" />
                <inertia ixx="2.29E-06" ixy="1.43E-06" ixz="0" iyy="4.56E-06" iyz="0" izz="4.77E-06" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/right_finger_prox_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/right_finger_prox_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}right_finger_prox_link">
            <material>Kortex/Gray</material>
        </gazebo>
        
        <joint name="${prefix}right_finger_bottom_joint" type="revolute">
            <origin xyz="0 -0.030501 0.070003" rpy="0 1.5708 0" />
            <parent link="${ns}${prefix}gripper_base_link" />
            <child link="${ns}${prefix}right_finger_prox_link" />
            <axis xyz="0 0 1" />
            <limit lower="-0.09" upper="0.96" effort="10" velocity="0.6" />
            <dynamics damping="0.1" friction="1"/>
        </joint>

        <link name="${ns}${prefix}right_finger_dist_link">
            <inertial>
                <origin xyz="0.018488 0.0011091 0" rpy="0 0 0" />
                <mass value="0.010748" />
                <inertia ixx="6.3E-07" ixy="3.2E-07" ixz="0" iyy="1.83E-06" iyz="0" izz="1.61E-06" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/right_finger_dist_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0 0.055 0.525 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/right_finger_dist_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}right_finger_dist_link">
            <material>Kortex/Blue</material>
        </gazebo>

        <joint name="${prefix}right_finger_tip_joint" type="revolute">
            <mimic joint="${prefix}right_finger_bottom_joint" multiplier="1"/>
            <origin xyz="-0.045636 0.020423 0" rpy="0 0 0" />
            <parent link="${ns}${prefix}right_finger_prox_link" />
            <child link="${ns}${prefix}right_finger_dist_link" />
            <axis xyz="0 0 1" />
            <limit lower="-0.50" upper="0.21" effort="1000" velocity="1000" /> <!--limit lower="-1.03" upper="0.21" effort="1000" velocity="1000" /-->
        </joint>

        <link name="${ns}${prefix}left_finger_prox_link">
            <inertial>
                <origin xyz="0.020257 0.0063483 6.99E-05" rpy="0 0 0" />
                <mass value="0.018385" />
                <inertia ixx="2.29E-06" ixy="1.43E-06" ixz="0" iyy="4.56E-06" iyz="0" izz="4.77E-06" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/left_finger_prox_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0.803 0.824 0.820 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/left_finger_prox_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}left_finger_prox_link">
            <material>Kortex/Gray</material>
        </gazebo>

        <joint name="${prefix}left_finger_bottom_joint" type="revolute">
            <mimic joint="${prefix}right_finger_bottom_joint" multiplier="-1"/>
            <origin xyz="0 0.0305 0.070003" rpy="0 1.5708 0" />
            <parent link="${ns}${prefix}gripper_base_link" />
            <child link="${ns}${prefix}left_finger_prox_link" />
            <axis xyz="0 0 1" />
            <limit lower="-0.96" upper="0.09" effort="10" velocity="0.6" />
            <dynamics damping="0.1" friction="1"/>
        </joint>

        <link name="${ns}${prefix}left_finger_dist_link">
            <inertial>
                <origin xyz="-0.018488 0.0011091 0" rpy="0 0 0" />
                <mass value="0.010748" />
                <inertia ixx="6.3E-07" ixy="3.2E-07" ixz="0" iyy="1.83E-06" iyz="0" izz="1.61E-06" />
            </inertial>
            <visual>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/left_finger_dist_link.STL" />
                </geometry>
                <material name="">
                    <color rgba="0 0.055 0.525 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                    <mesh filename="file://$(find jackal_description)/meshes/gen3lite/left_finger_dist_link.STL" />
                </geometry>
            </collision>
        </link>
        <gazebo reference="${ns}${prefix}left_finger_dist_link">
            <material>Kortex/Blue</material>
        </gazebo>

        <joint name="${prefix}left_finger_tip_joint" type="fixed">
            <mimic joint="${prefix}right_finger_bottom_joint" multiplier="-1"/>
            <origin xyz="-0.045636 -0.020423 6.9901E-05" rpy="0 0 0" />
            <parent link="${ns}${prefix}left_finger_prox_link" />
            <child link="${ns}${prefix}left_finger_dist_link" />
            <axis xyz="0 0 -1" />
            <limit lower="-0.50" upper="0.21" effort="1000" velocity="1000" /> <!--limit lower="-1.03" upper="0.21" effort="1000" velocity="1000" /-->
        </joint>

    </xacro:macro>

</robot>